{
  "manifest_version": 3,
  "name": "Prompt Organizer",
  "version": "1.0",
  "description": "Save, tag, and organize your AI prompts easily.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  }
}
